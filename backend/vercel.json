{
    "version": 2,
    "builds": [
        {
            "src": "index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "./Foodmine/backend/src/index.ts"
        },
        {
            "src": "/",
            "dest": "./Foodmine/backend/src/index.ts"
        }
    ]
}
